# UNIX
alias c="clear"
alias e="$EDITOR"
alias r="source ~/.zshrc"
alias j="jump"
alias grep="grep -G"
alias path='echo -e ${PATH//:/\\n}'
alias ctsso="$HOME/internal-tools/control_tower_sso/x86_control_tower_sso/control_tower_sso"
alias ll='ls -laF'

alias g='git'

# others
alias brew='env PATH=${PATH//$(pyenv root)\/shims:/} brew'
alias cat='bat'

# dbt
alias dbt_clean='dbt clean ; dbt deps'
alias dbt_docs='dbt docs generate && dbt docs serve'
